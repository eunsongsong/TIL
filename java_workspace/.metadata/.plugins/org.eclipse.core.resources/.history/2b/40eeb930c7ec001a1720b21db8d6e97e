package workshop.animal.entity;

public class Fish extends Animal implements Pet {
	public String name;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	@Override
	public void play() {
		System.out.println();
	}
	
	@Override
	public void eat() {
		System.out.println("물고기는 물 속에서 먹이를 먹는다.");
	}
	
	@Override
	public void walk() {
		System.out.println("물고기는 헤엄친다.");
	}

	
}
