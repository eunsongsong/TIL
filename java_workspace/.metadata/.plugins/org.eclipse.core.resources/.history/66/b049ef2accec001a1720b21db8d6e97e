package workshop.animal.entity;

public class Cat extends Animal implements Pet{
	public String name;
	


	@Override
	public void play() {
		System.out.println("고양이는 쥐를 잡는다.");
		
	}

	@Override
	public void eat() {
		System.out.println();
	}

	@Override
	public String setName(String name) {
		return name;
	}

	@Override
	public String getName() {
		return name;
	}

	
		

}
