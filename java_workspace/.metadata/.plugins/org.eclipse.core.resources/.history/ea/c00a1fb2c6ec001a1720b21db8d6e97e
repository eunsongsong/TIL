package workshop.animal.entity;

public class Cat extends Animal implements Pet{
	public String name;
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public void play() {
		System.out.println("고양이는 털뭉치를 좋아한다.");
		
	}

	@Override
	public void eat() {
		System.out.println("고양이는 츄르를 좋아한다.");
		
	}
		

}
